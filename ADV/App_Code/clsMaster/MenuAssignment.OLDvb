                End If

            Case "RUN_SPV"
                If Path.ContainsAny(AllowedMenuSPVRun) Then
                    Return True
                End If

            Case "RUN_SPVMONCEN"
                If Path.ContainsAny(AllowedMenuSPVMoncen) Then
                    Return True
                End If
        End Select

        'Role SIMULATOR
        Select Case Role
            Case "ASS_OP"
                If Path.ContainsAny(AllowedMenuOPSimulator) Then
                    Return True
                End If

        End Select

        'Role CPC
        Select Case Role
            Case "CPC_OPR"
                If Path.ContainsAny(AllowedMenuOprCPC) Then
                    Return True
                End If

            Case "CPC_SPV"
                If Path.ContainsAny(AllowedMenuSpvCPC) Then
                    Return True
                End If
        End Select

        'Role BBM
