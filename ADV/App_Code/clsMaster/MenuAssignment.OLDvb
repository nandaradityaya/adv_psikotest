        'Role RQC
        Select Case Role
            Case "RQC_OP"
                If Path.ContainsAny(AllowedMenuOperatorRQC) Then
                    Return True
                End If

            Case "RQC_ADM"
                If Path.ContainsAny(AllowedMenuAdminRQC) Then
                    Return True
                End If
            Case "RQC_AUDIT"
                If Path.ContainsAny(AllowedMenuAuditRQC) Then
                    Return True
                End If
        End Select

        Return False
